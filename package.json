{
  "name": "testcafe-example",
  "version": "1.0.0",
  "description": "An example project running e2e tests with TestCafe",
  "engines": {
    "node": "12.13.0"
  },
  "main": "index.js",
  "scripts": {
    "testcafe": "./node_modules/.bin/testcafe",
    "test": "npm run testcafe chrome examples/ready-to-go/tests/",
    "parallel": "npm run testcafe -- -c 2 chrome examples/ready-to-go/tests/",
    "headless parallel": "npm run testcafe -- -c 2 chrome:headless examples/ready-to-go/tests/",
    "safari": "npm run testcafe safari examples/ready-to-go/tests/",
    "firefox": "npm run testcafe firefox examples/ready-to-go/tests/",
    "tasks": "npm run testcafe chrome tasks/",
    "tasks parallel": "npm run testcafe -- -c 2 chrome tasks/"
  },
  "author": "Brine",
  "license": "MIT",
  "dependencies": {
    "minimist": "^1.2.0",
    "node-fetch": "^2.6.0",
    "testcafe": "^1.8.2"
  }
}
